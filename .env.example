# API Configuration
VITE_AUTH_API_URL= your_authentication_api_url
VITE_GALLERY_API_URL= your_authentication_api_url
VITE_USER_API_URL= your_authentication_api_url

# Authentication
VITE_AUTH_TOKEN_KEY=kabbala_auth_token
VITE_REFRESH_TOKEN_KEY=kabbala_refresh_token
VITE_DEVICE_TOKEN_KEY=kabbala_device_token

# Storage Configuration
VITE_IMAGE_STORAGE_URL= your_authentication_api_url
